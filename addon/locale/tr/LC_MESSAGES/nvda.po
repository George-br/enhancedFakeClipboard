#. Translations: Message to announce select all action.
#: addon\globalPlugins\enhancedFakeClipboard.py:29
msgid "Select all"
msgstr "Tümünü seç"

#. Translations: Message to announce cut action.
#: addon\globalPlugins\enhancedFakeClipboard.py:31
msgid "Cut"
msgstr "Kes"

#. Translations: Message to announce copy action.
#: addon\globalPlugins\enhancedFakeClipboard.py:33
msgid "Copy"
msgstr "Kopyala"

#. Translations: Message to announce paste action.
#: addon\globalPlugins\enhancedFakeClipboard.py:35
msgid "Paste"
msgstr "Yapıştır"

#. Translations: Message displayed when no text selected.
#: addon\globalPlugins\enhancedFakeClipboard.py:77
msgid "no selection"
msgstr "Seçim yok"

#. Translations: Message displayed when no files selected.
#: addon\globalPlugins\enhancedFakeClipboard.py:103
msgid "No file selected"
msgstr "Seçili dosya yok"

#. Translations: Message to denote that there are no files in clipboard.
#: addon\globalPlugins\enhancedFakeClipboard.py:108
msgid "No files in clipboard"
msgstr "Panoda dosya yok"

